```python
# Code here !!...

# Save the results to a CSV file
df.to_csv("Student_report_copy.csv", index=False)

# Create a report (e.g., in a Markdown file)
with open("report.md", "w") as f:
    f.write("## Student_report Dataset Analysis\n\n")
    # ... (Report content here)
    f.write("# Data Loading\n")
    f.write("import pandas as pd # pandas library\n")
    f.write("df = pd.read_csv(Student_report.csv) # Load the Student_report dataset from a CSV file\n\n")
    f.write("# Data Cleaning\n")
    f.write("df.columns # Column names\n")
    f.write("df.rename(columns={'Roll_number': 'Roll', 'Student_name': 'Name', 'Subject_name': 'Subject', 'Student_performances': 'Performance'}, inplace=True) # Rename column\n")
    f.write("df.head(10) # Head (Show top values)\n")
    f.write("df.tail(10) # Tail (Show top values)\n")
    f.write("duplicated_value = df.duplicated() # Check for duplicates in each row\n")
    f.write("df.drop_duplicates(inplace=True) # Remove duplicates if any\n")
    f.write("missing_values = df.isnull().sum() # Check for missing values in each column\n")
    f.write("a = df.fillna(0) # Fill missing values with 0\n")
    f.write("b = df.dropna() # Drop rows with missing values\n\n")
    f.write("# Data Manipulation\n")
    f.write("df.size # Size\n")
    f.write("df.shape # Shape\n")
    f.write("df.index # Index\n")
    f.write("df.dtypes # Data type\n")
    f.write("df['Percentage'] = (df['Total']/900) * 100 # New column value\n")
    f.write("df.__array__ # Data type array\n")
    f.write("df['Name'].head(10) # Filter data by student name\n")
    f.write("df_pass = df[df['Student_performances'] == 'Pass'] # Show pass students data\n")
    f.write("df_fail = df[df['Student_performances'] == 'Fail'] # Show fail students data\n")
    f.write("grouped_by_Student_performances = df.groupby('Student_performances') # Group by student\n")
    f.write("grouped_by_Performance.first() # Show only one student data of both category\n")
    f.write("# Data Analysis\n")
    f.write("import matplotlib.pyplot as plt # matplotlib library\n")
    f.write("import seaborn as sns # seaborn library\n")
    f.write("statistics = df.describe() # Statistics\n")
    f.write("* Bar\n")
    f.write("plt.figure(figsize=(19000, 1200)) # Figure size\n")
    f.write("df.plot.barh('Name','Percentage', color='b'', fontsize = 5)  # Horizontal bars with blue color\n")
    f.write("plt.grid(axis='x', color='r') # Add grid lines on the y-axis with red color\n")
    f.write("plt.xticks(rotation=90) # Rotate x-axis labels by 90 degrees for better readability\n")
    f.write("plt.title('\n Percentage of Students\n', fontsize=40)\n")
    f.write("plt.ylabel('Student Name', fontsize=30)\n")
    f.write("plt.xlabel('Percentage', fontsize=30)\n")
    f.write("plt.show() # Display the plot\n")
    f.write("* PIE\n")
    f.write("df_filtered = df.dropna(subset=['Performance'])  # Remove rows with missing values in Performance")
    f.write("chart = df_filtered['Performance'].value_counts().sort_values(ascending=False)  # Count occurrences")
    f.write("plt.pie(chart, labels=chart.index.to_list(), autopct=%1.1f%%)  # Add percentages to labels")
    f.write("plt.title(Showing Performance Statistics of Student, color='red')")
    f.write("plt.show()")
    f.write("* Plotting\n")
    f.write("sns.pairplot(df)\n")
    f.write("plt.show()\n")
```


